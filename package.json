{
	"name": "@remunda/jira-migrate",
	"version": "1.0.0",
	"description": "CLI tool for migrating JIRA items (Epics, Features, User Stories) to Shortcut.com or ClickUp",
	"main": "dist/index.js",
	"bin": {
		"jira-migrate": "dist/cli.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/cli.js",
		"dev": "ts-node src/cli.ts",
		"test": "jest",
		"precommit": "biome format --fix && biome lint --fix && biome check --fix",
		"prepublishOnly": "yarn build"
	},
	"keywords": [
		"jira",
		"shortcut",
		"clickup",
		"migration",
		"cli",
		"atlassian",
		"import",
		"export"
	],
	"author": "remunda@gmail.com",
	"license": "CC-BY-SA-4.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/remunda/jira-migrate.git"
	},
	"bugs": {
		"url": "https://github.com/remunda/jira-migrate/issues"
	},
	"homepage": "https://github.com/remunda/jira-migrate#readme",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"axios": "^1.6.0",
		"chalk": "^4.1.2",
		"commander": "^11.1.0",
		"dotenv": "^16.3.1",
		"form-data": "^4.0.4",
		"inquirer": "^9.2.12",
		"ora": "^5.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/form-data": "^2.5.2",
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.19.19",
		"jest": "^29.7.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"packageManager": "yarn@4.9.2"
}

		"form-data": "^4.0.4",
		"inquirer": "^9.2.12",
		"ora": "^5.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/form-data": "^2.5.2",
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.19.19",
		"jest": "^29.7.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"

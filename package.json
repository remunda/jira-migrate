{
	"name": "shortcut-jira",
	"version": "1.0.0",
	"description": "CLI tool for migrating JIRA items (Epics, Features, User Stories) to Shortcut.com",
	"main": "dist/index.js",
	"bin": {
		"jira-to-shortcut": "dist/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/cli.js",
